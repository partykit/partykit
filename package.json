{
  "name": "@partykit/repo",
  "version": "0.0.0",
  "private": true,
  "description": "The monorepo for Partykit",
  "workspaces": [
    "apps/*",
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "start": "npm start -w partykit",
    "build": "npm run build -w partykit -w y-partykit -w partysocket -w partykit.io -w create-partykit -w partymix -w @partykit/site -w @partykit/blog -w @partykit/docs",
    "check": "npm run lint && npm run repocheck && npm run typecheck && npx vitest --watch=false",
    "lint": "npx eslint \"**/*.[tj]s?(x)\" --max-warnings=0 --report-unused-disable-directives",
    "test": "vitest",
    "repocheck": "manypkg check",
    "typecheck": "npm exec tsc --workspaces",
    "coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "PartyKit, Inc. <team@partykit.io>",
  "license": "ISC",
  "dependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.2",
    "@manypkg/cli": "^0.21.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@vitest/coverage-c8": "^0.33.0",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.47.0",
    "eslint-plugin-deprecation": "^1.5.0",
    "ink-testing-library": "^3.0.0",
    "jsdom": "^22.1.0",
    "shx": "^0.3.4",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "vitest": "^0.34.1"
  },
  "trustedDependencies": [
    "sharp"
  ]
}
