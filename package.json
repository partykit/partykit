{
  "name": "@partykit/repo",
  "version": "0.0.0",
  "private": true,
  "description": "The monorepo for Partykit",
  "packageManager": "pnpm@8.15.1",
  "scripts": {
    "start": "pnpm --filter ./packages/partykit run start",
    "build": "pnpm --filter \"./packages/**\" run build",
    "check": "concurrently \"npm run repocheck\" \"npm run lint\" \"npm run typecheck\" \"npx vitest --watch=false\" --kill-others-on-fail",
    "lint": "npx eslint \"**/*.[tj]s?(x)\" --max-warnings=0 --report-unused-disable-directives",
    "test": "vitest",
    "repocheck": "pnpm dlx sherif@latest",
    "typecheck": "npm exec tsc --workspaces",
    "postinstall": "pnpm repocheck"
  },
  "keywords": [],
  "author": "PartyKit, Inc. <team@partykit.io>",
  "license": "ISC",
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@microsoft/api-extractor": "^7.40.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "esbuild-register": "^3.5.0",
    "eslint": "^8.56.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "ink-testing-library": "^3.0.0",
    "jsdom": "^24.0.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.2.5",
    "shx": "^0.3.4",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2",
    "esbuild": "0.20.0"
  },
  "trustedDependencies": [
    "sharp"
  ],
  "pnpm": {
    "patchedDependencies": {
      "@cloudflare/ai@1.0.50": "patches/@cloudflare+ai+1.0.50.patch",
      "@astrojs/starlight@0.17.4": "patches/@astrojs+starlight+0.17.4.patch"
    }
  }
}
